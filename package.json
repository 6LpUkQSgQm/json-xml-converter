{
  "name": "json-xml-converter",
  "version": "1.0.1",
  "description": "A simple CLI to convert json file to xml file",
  "types": "./lib/index.d.ts",
  "bin": {
    "json-xml-converter": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" src/index.ts",
    "build": "tsc -p .",
    "local": "sudo npm i -g && json-xml-converter",
    "refresh": "rm -rf ./node_modules ./package-lock.json && yarn install",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/6LpUkQSgQm/json-xml-converter.git"
  },
  "keywords": [
    "CLI",
    "json",
    "to",
    "xml",
    "xml",
    "json",
    "converter",
    "convert"
  ],
  "author": "6LpUkQSgQm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/6LpUkQSgQm/json-xml-converter/issues"
  },
  "homepage": "https://github.com/6LpUkQSgQm/json-xml-converter#readme",
  "dependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^9.5.0",
    "eslint": "^8.31.0",
    "fast-xml-parser": "^4.0.15",
    "figlet": "^1.5.2",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "path": "^0.12.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
